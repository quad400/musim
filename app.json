{
  "expo": {
    "name": "musim",
    "slug": "musim",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.coderblack.musim"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "newArchEnabled": false,
      "package": "com.coderblack.musim"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#000"
        }
      ],
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/Sora-Bold.ttf",
            "./assets/fonts/Sora-Regular.ttf",
            "./assets/fonts/Sora-SemiBold.ttf",
            "./assets/fonts/Sora-Light.ttf",
            "./assets/fonts/Sora-Medium.ttf",
            "./assets/fonts/Sora-ExtraBold.ttf"
          ]
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
